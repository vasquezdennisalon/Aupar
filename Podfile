# Uncomment the next line to define a global platform for your project
platform :ios, '12.2'

target 'Aupar' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Aupar
  # Network
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'SwiftyJSON'
  
  # Storage
  pod 'RealmSwift'

  # Reactive
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  
  # Custom Design

  target 'AuparTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AuparUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
